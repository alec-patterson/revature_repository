package dogSim;

public class dogSim {
	public static void main(String[] args) {
		dog d = new dog("queenie", 13);
		System.out.println(d);
	}
}

class dog {
	public String name;
	public int age;
	
	public dog(String n, int a) {
		name = n;
		age = a;
	}
	
	public void bark() {
		System.out.println("Woof Woof");
	}
	
	public String toString() {
		return "Name: " + name + "\nAge: " + age;
	}
}
